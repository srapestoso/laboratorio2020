#include <stdio.h>
#include <stdlib.h>

int main () {

	setbuf(stdout, NULL);
   int n;

   printf("Ingrese un numero");
   scanf("%d",&n);

   if(n<=500 && n>=200){
	   printf("Es válido");
   }
   else
   {
	   printf("No cumple la validación");
   }

   return 0;
}
