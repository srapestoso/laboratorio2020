#ifndef ARRAYEMPLOYEE_H_
#define ARRAYEMPLOYEE_H_

typedef struct{
	int isEmpty;
	int estado;
	int kilosTotales;
	int cantidad;
}Residuos;

typedef struct{
	int id;
	int cuit;
	char direcccion[21];
	char localidad[21];
	int isEmpty;
}Cliente;

#endif /* ARRAYEMPLOYEE_H_ */
